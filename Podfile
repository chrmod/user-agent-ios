platform :ios, '11.4'

project './Client.xcodeproj'

inhibit_all_warnings!
use_frameworks!

target 'Cliqz' do
  pod 'SnapKit', '~> 5.0.0', :modular_headers => true
  pod 'SDWebImage', '~> 5.0', :modular_headers => true
end

target 'Lumen' do
  pod 'SnapKit', '~> 5.0.0', :modular_headers => true
  pod 'SDWebImage', '~> 5.0', :modular_headers => true
end

target 'Ghostery' do
  pod 'SnapKit', '~> 5.0.0', :modular_headers => true
  pod 'SDWebImage', '~> 5.0', :modular_headers => true
end

target 'Storage' do
  pod 'SDWebImage', '~> 5.0', :modular_headers => true
  pod 'SnapKit', '~> 5.0.0', :modular_headers => true
end

target 'ShareTo' do
  pod 'SnapKit', '~> 5.0.0', :modular_headers => true
end

target 'Shared' do
  pod 'SDWebImage', '~> 5.0', :modular_headers => true

  pod 'Sentry', :git => 'https://github.com/getsentry/sentry-cocoa.git', :tag => '4.3.1'
  pod 'SwiftLint'
end
